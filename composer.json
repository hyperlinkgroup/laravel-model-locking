{
  "name": "hyperlinkgroup/laravel-model-locking",
  "description": "Wordpress like locking feature for Laravel-Models.",
  "type": "library",
  "license": "MIT",
  "require": {
	"php": "^8.0"
  },
  "require-dev": {
	"roave/security-advisories": "dev-latest",
	"orchestra/testbench": "7.0",
    "pestphp/pest-plugin-laravel": "^1.3"
  },
  "autoload": {
	"psr-4": {
	  "Hylk\\Locking\\": "src/"
	}
  },
  "autoload-dev": {
	"psr-4": {
	  "Hylk\\Locking\\": "tests/"
	}
  },
  "extra": {
	"laravel": {
	  "providers": [
		"Hylk\\Locking\\ModelLockingServiceProvider"
	  ]
	}
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
